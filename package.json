{
  "name": "catstack-log",
  "version": "0.0.0",
  "description": "catstack module for logging",
  "main": "index.js",
  "scripts": {
    "start": "node-dev example",
    "test:deps": "dependency-check . && dependency-check . --extra --no-dev -i es2040",
    "test:lint": "standard",
    "test:node": "NODE_ENV=test run-default tape test/*.js --",
    "test:coverage": "NODE_ENV=test nyc npm run test:node",
    "test:coverage:report": "NODE_ENV=test nyc report --reporter=text-lcov npm run test:node",
    "test": "npm-run-all -s test:node test:lint test:deps"
  },
  "browserify": {
    "transform": [
      "es2040"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/catstack-log.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/catstack-log/issues"
  },
  "homepage": "https://github.com/ahdinosaur/catstack-log#readme",
  "devDependencies": {
    "dependency-check": "^2.7.0",
    "depject": "^3.1.5",
    "node-dev": "^3.1.3",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "run-default": "^1.0.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "es2040": "^1.2.3",
    "object-assign": "^4.1.1",
    "pino": "^3.2.0"
  }
}
